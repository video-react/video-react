    "react-router": "^2.6.1",
    "react-scripts": "^0.7.0",
    "reactstrap": "^4.0.1",
    "redux": "^3.6.0",
    "sass-loader": "^4.0.2",
    "sinon": "^1.17.6",
    "static-site-generator-webpack-plugin": "^2.0.1",
    "style-loader": "^0.13.1",
